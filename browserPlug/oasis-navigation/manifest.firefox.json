{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "1.0.1",
  "description": "__MSG_extensionDescription__",
  "default_locale": "zh_CN",
  "author": "tan",
  "homepage_url": "https://gitee.com/tanoo/oasis.git",
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "cookies",
    "downloads",
    "https://pan.baidu.com/*",
    "https://c2.pcs.baidu.com/*",
    "https://*.baidupcs.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "oasis-navigation@tannn.cn",
      "strict_min_version": "140.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_title": "__MSG_extensionTitle__"
  }
}
